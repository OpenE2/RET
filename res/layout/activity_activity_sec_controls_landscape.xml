<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal"
    android:baselineAligned="false"
    android:contentDescription="@string/discription">


	<TableLayout
	    android:layout_width="0dp"
	    android:layout_weight="1"
	    android:layout_height="fill_parent"
	    android:orientation="vertical">
	
	<TableRow android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:id="@+id/first_row"
        android:layout_weight="1">
    
	    <TextView android:id="@+id/con_forward"
	        android:hint="@string/con_forward"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border"
        	android:tag="@string/con_com_forward"/>	 
        	
	    <TextView android:id="@+id/con_play"
	        android:hint="@string/con_play"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border"
        	android:tag="@string/con_com_play"/>	
        	
	    <TextView android:id="@+id/con_rewind"
	        android:hint="@string/con_rewind"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border"
        	android:tag="@string/con_com_rewind"/>	
	</TableRow>

    <TableRow android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:id="@+id/sec_row"
        android:layout_weight="1">
    
	    <TextView android:id="@+id/con_next"
	        android:hint="@string/con_next"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
	    	android:background="@drawable/border_right_bottom_left"
	    	android:tag="@string/con_com_next"/>
	   
	    <TextView android:id="@+id/con_pause"
	        android:hint="@string/con_pause"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom"
        	android:tag="@string/con_com_pause"/>
	    
	    <TextView android:id="@+id/con_stop"
	        android:hint="@string/con_stop"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom"
        	android:tag="@string/con_com_stop"/>
	    
    </TableRow>
    
    <TableRow android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:id="@+id/third_row"
        android:layout_weight="1">
    
	    <TextView android:id="@+id/con_previous"
	        android:hint="@string/con_previous"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom_left"
        	android:tag="@string/con_com_previous"/>
	    
	    <TextView android:id="@+id/con_rec"
	        android:hint="@string/con_rec"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom"
        	android:tag="@string/con_com_rec"/>
	    
	    <TextView android:id="@+id/con_help"
	        android:hint="@string/con_help"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom"
        	android:tag="@string/con_com_help"/>
    
