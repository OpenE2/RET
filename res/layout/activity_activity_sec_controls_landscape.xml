	    <TextView android:id="@+id/con_right"
	        android:hint="@string/con_right"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom"
        	android:tag="@string/con_com_right"/>
	        
	</TableRow>
	
	<TableRow android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:id="@+id/eighth_row"
        android:layout_weight="1">
    
	    <TextView android:id="@+id/con_tv"
	        android:hint="@string/con_tv"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:background="@drawable/border_right_bottom_left"
	        android:clickable="true"
        	android:onClick="intent"
        	android:tag="@string/con_com_tv"/>
	    
	    <TextView android:id="@+id/con_down"
	        android:hint="@string/con_down"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="3"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom_left"
        	android:tag="@string/con_com_down"/>

