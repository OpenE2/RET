        android:orientation="horizontal"
        android:id="@+id/fourth_row"
        android:layout_weight="1">
    
	    <TextView android:id="@+id/con_audio"
	        android:hint="@string/con_audio"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom_left"
        	android:tag="@string/con_com_audio"/>
	    
	    <TextView android:id="@+id/con_video"
	        android:hint="@string/con_video"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom"
        	android:tag="@string/con_com_video"/>
	    
	    <TextView android:id="@+id/con_epg"
	        android:hint="@string/con_epg"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom"
        	android:tag="@string/con_com_epg"/>
   
    </TableRow>
    
    <TableRow android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:id="@+id/fifth_row"
        android:layout_weight="1">
    	
        <TextView android:id="@+id/con_menu"
	        android:hint="@string/con_menu"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom_left"
        	android:tag="@string/con_com_menu"/>
        
