	<TableRow android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:id="@+id/ninth_row"
        android:layout_weight="1">
	    
	    <TextView android:id="@+id/con_radio"
	        android:hint="@string/con_radio"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:background="@drawable/border_right_bottom_left"
	        android:clickable="true"
        	android:onClick="intent"
        	android:tag="@string/con_com_radio"/>
    
	    <TextView android:id="@+id/con_vol_dec"
	        android:hint="@string/con_vol_dec"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:background="@drawable/border_right_bottom_left"
	        android:clickable="true"
        	android:onClick="intent"
        	android:tag="@string/con_com_vol_dec"/>
	    
	    <TextView android:id="@+id/con_vol_mute"
	        android:hint="@string/con_vol_mute"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:background="@drawable/border_right_bottom"
	        android:clickable="true"
        	android:onClick="intent"
        	android:tag="@string/con_com_mute"/>
	    
	    <TextView android:id="@+id/con_vol_inc"
	        android:hint="@string/con_vol_inc"
	        android:layout_width="0dp"
	        android:layout_height="fill_parent"
	        android:gravity="center"
	        android:textSize="@dimen/font_size_main"
	        android:layout_weight="1"
	        android:clickable="true"
        	android:onClick="intent"
        	android:background="@drawable/border_right_bottom"
